/// <reference path="../types/momoa.d.ts" />
import { Ctx, GeoJSONTypeSet } from './types';
import { Node } from '@humanwhocodes/momoa';
export declare function getType(ctx: Ctx, node: Node, allowedTypes: GeoJSONTypeSet): {
    type?: undefined;
    objectNode?: undefined;
} | {
    type: "Point" | "MultiPoint" | "LineString" | "MultiLineString" | "Polygon" | "MultiPolygon" | "GeometryCollection" | "Feature" | "FeatureCollection";
    objectNode: import("@humanwhocodes/momoa").ObjectNode;
};
